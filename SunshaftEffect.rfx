    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 150 compatibility

in vec2 UV_VS;
uniform sampler2D modelnsun;
uniform sampler2D classic;

uniform vec2 fViewPortDim;
uniform float NUM_SAMPLES;
uniform vec2 sunPos;
uniform float density;
uniform float weight;
uniform float decay;
uniform float exposure;

//Passe d'un poitn dfini entre [Width,Height]  un point dfini entre [-1,1]
vec2 convertToRepere1(in vec2 toConvert){
   return (toConvert/fViewPortDim-0.5)*2.0;
}
//Passe d'un point dfini entre [-1,1]  un point dfini entre [Width,Height]
vec2 convertToRepereScreen(in vec2 toConvert){
   vec2 tempo = (toConvert/2)+0.5;
   return tempo*fViewPortDim;
}

void main(void)
{
   gl_FragColor=texture(classic,gl_TexCoord[0].st);
   vec2 lightScreenPos =(sunPos+vec2(1.0,1.0))/2;
  /* if(gl_FragCoord.x<lightScreenPos.x)
   {
      gl_FragColor=gl_FragColor+vec4(1.0,0.0,0.0,1.0);
   }
   if(gl_FragCoord.y<lightScreenPos.y){
      gl_FragColor=gl_FragColor+vec4(0.0,1.0,0.0,1.0);
   }*/
   vec2 deltaTextCoord = vec2( gl_TexCoord[0].st - lightScreenPos.xy );
   vec2 textCoo = gl_TexCoord[0].st;
   deltaTextCoord *= 1.0 /  float(NUM_SAMPLES) * density;
   float illuminationDecay = 1.0;


   for(int i=0; i < NUM_SAMPLES ; i++)
    {
       textCoo -= deltaTextCoord;
       vec4 sample = texture2D(modelnsun, textCoo );

       sample *= illuminationDecay * weight;

       gl_FragColor += sample;

       illuminationDecay *= decay;
     }
    gl_FragColor *= exposure;
  
}]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 150 compatibility

out vec2 UV_VS;

void main(void)
{
   
   gl_Position = gl_Vertex;
   UV_VS = (gl_Position.xy+vec2(1.0,1.0))/2.0;
   //gl_TexCoord[0] = vec4(UV_VS,0.0,0.0);
   gl_TexCoord[0] = vec4(UV_VS,0.0,0.0);
}]]></RmShaderCode></RmGLShader>
