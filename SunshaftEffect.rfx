    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 150 compatibility
out vec4 color;

in vec3 L_VS;
in vec3 N_VS;
uniform vec2 sunPos;
uniform vec2 view;


/////////////////////////////
//PHONG PARAMS
uniform vec3 lightPos;
uniform vec4 Id;
uniform vec4 V;
uniform vec4 Is;
uniform vec4 kd;
uniform float alphaSpec;
uniform vec4 Ia;
uniform vec4 ka;
uniform vec4 ks;
uniform float ambiantCoeff;
uniform float diffusCoeff;
uniform float specCoeff;
////////////////////////////



vec4 calculateAmbiant(){
   return Ia * ka;
}

vec4 calculateSpec(in vec3 L, in vec3 N){
   vec3 R = normalize(reflect(L,N));
   return Is*ks*pow(max(dot(R,V.xyz),0.0),alphaSpec);
}

vec4 calculateDiffus(in vec3 L, in vec3 N){
   return Id*kd*max(dot(N,L),0.0);
}

vec4 calculatePhong(in vec3 L, in vec3 N){   
   return calculateAmbiant()*ambiantCoeff+calculateDiffus(L,N)*diffusCoeff+calculateSpec(L,N)*specCoeff;
}

void main(void){
   color = calculatePhong(normalize(L_VS),normalize(N_VS));
}]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 150 compatibility

out vec3 L_VS;
out vec3 N_VS;

uniform vec2 sunPos;
uniform vec2 fViewPortDim;
uniform float lightPosZ;
uniform mat4 mvp;
uniform mat4 mvpInverse;
uniform mat4 projectionInverseMat;
uniform mat4 viewInverseMat;
uniform mat4 mv;
//Passe d'un point dfini entre [-1,1]  un point dfini entre 
// [-Width/2,-Height/2][Width/2,Height/2]
vec2 convertToRepereScreen(in vec2 toConvert){
   vec2 maxValues = fViewPortDim/2;
   return toConvert*maxValues;
}
void main(void)
{
   gl_Position = mvp*gl_Vertex;
   vec4 lightPos = vec4(convertToRepereScreen(sunPos),lightPosZ,1.0);
   lightPos = mv*lightPos;
   L_VS = -normalize(gl_Position.xyz - lightPos.xyz);
   N_VS = gl_Normal.xyz;
}]]></RmShaderCode></RmGLShader>
