#version 150 compatibility

uniform mat4 model;
uniform mat4 view;
uniform mat4 projection;
uniform mat4 mvp;
uniform mat4 overX_90;
uniform vec2 fViewPortDim;
uniform vec2 sunPos;

///////////////////////////
// ARTIST VARIABLES
///////////////////////////
uniform float sunSize;

void main(void)
{
   mat4 matB = mat4(
      vec4(sunSize,0.0,0.0,0.0),
      vec4(0.0,sunSize,0.0,0.0),
      vec4(0.0,0.0,sunSize,0.0),
      vec4(0.0,0.0,0.0,1.0)
   );
   mat4 viewR = view*matB;
   gl_Position = projection*viewR*model*overX_90*gl_Vertex;
   vec2 viewPortDisplacementAmount = sunPos*fViewPortDim/2.0*-1.0;
   gl_Position = gl_Position - vec4(viewPortDisplacementAmount,0.0,0.0);
   //gl_Position = mvp*overX_90*gl_Vertex;
}]]></RmShaderCode></RmGLShader>
